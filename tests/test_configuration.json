{
  "model": {
    "backbone": "resnet18",
    "pretrained": true,
    "freeze": true,
    "cut": -1,
    "dropout": 0.5,
    "hidden_units": 512,
    "device": "cpu"
  },
  "train": {
    "epochs": 10,
    "batch_size": 32,
    "learning_rate": 0.001,
    "momentum": 0.9,
    "weight_decay": 0.0001,
    "device": "cpu"
  }
}