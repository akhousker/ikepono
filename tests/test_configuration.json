{
  "model": {
    "backbone": "resnet18",
    "pretrained": true,
    "freeze": true,
    "cut": -1,
    "dropout": 0.5,
    "backbone_output_dim": 512,
    "hidden_units": 512,
    "output_vector_size": 128,
    "dataset_device": "cpu",
    "model_device": "cuda"
  },
  "train": {
    "epochs": 10,
    "batch_size": 32,
    "learning_rate": 0.001,
    "momentum": 0.9,
    "weight_decay": 0.0001,
    "num_classes": 10,
    "optimizer" : "adam",
    "criterion": "SubCenterArcFaceLoss",
    "dataset_device": "cpu",
    "model_device": "cuda"
  }
}